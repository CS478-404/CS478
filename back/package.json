{
  "scripts": {
    "setup": "rm -f database.db && sqlite3 database.db < setup.sql",
    "build": "mkdir -p dist && rm -rf dist/* && tsc && mkdir dist/public && (cd ../front && npm run build) && cp -r ../front/dist/* dist/public",
    "start": "cd dist && node server.js",
    "dev": "cd src && tsx --watch server.ts",
    "test": "vitest",
    "data": "tsx src/data.ts"
  },
  "type": "module",
  "dependencies": {
    "argon2": "^0.44.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "tsx": "^4.21.0"
  }
}
